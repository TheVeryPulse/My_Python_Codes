sentence = ("You can have data without information, but you cannot have information without data.")
sentence = sentence.lower()
letterlist = ['a','b','c','d','e','f','g','h','i','j','k','l','m','n','o','p','q','r','s','t','u','v','w','x','y','z']
print (sentence)
for i in letterlist:
  freq = 0
  for j in sentence:
    if j == i:
      freq += 1
  print (i+' : '+str(freq))
